      </view>
      <view class='value'>
        {{event.zbf}}
      </view>
    </view>
  </view>
   

<!-- 参赛球队列表 -->
<view class='section mt-20'>
  <view class='section-title' >共{{event.zbf}}支参赛球队</view> 
  <view class='weui-cells weui-cells_after-title'>
    <navigator class="weui-cell weui-cell_access" url="../../team/detail/detail">
      <div class="weui-cell__hd"><image src="../../../../img/logo_v.jpg" alt="" class="avatar-sz25 circle"/></div>
      <div class="weui-cell__bd">
        {{event.zbf}}
      </div>
      <view class="tip">{{event.zbf}}</view> 
    </navigator>
  </view>
</view>
<!-- / 参赛球队列表 -->


</view>
