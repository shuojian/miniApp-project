        合作伙伴
      </view>
      <view class='value'>
        {{event.zbf}}
      </view>
    </view>
    <view class='kvpreview-item'>
      <view class='key'>
          起止日期
      </view>
      <view class='value'>
        {{event.zbf}}
      </view>
    </view>
    <view class='kvpreview-item'>
      <view class='key'>
         报名截止
      </view>
