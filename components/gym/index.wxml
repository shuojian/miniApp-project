<view bindtap='onTap' class="container bg-f border-b weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active">
  <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
      <image class="thumb" src="../../img/logo_v.jpg" />
  </view>
  <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
    <view class="weui-media-box__title">{{gym.gymName}}</view>
    <view class="weui-media-box__desc">
      {{gym.gymAddr}}
    </view>
    <view class='tags'>
      <view class='tag gray'>5人制</view>
    </view>
  </view>
</view>
